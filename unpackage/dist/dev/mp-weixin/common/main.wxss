.container {
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	box-sizing: border-box;
	background: #f8f8f8;
}
button::after {
	border: none;
}
.reach_bottom {
	text-align: center;
	line-height: 80rpx;
	font-size: 24rpx;
	color: #626262;
	background: #ffffff;
}

